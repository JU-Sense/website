</head>

<body>
  <!--Main Navigation-->
  <header>
    <!-- Navbar -->
    <%- include('../partials/navbar'); %>
    <!-- Navbar -->


    <!-- Background image -->
    <div class="p-5 text-center bg-image" style="background-image: url('./img/bg1.jpg'); height: 250px; margin-bottom: 50px;">
      <div class="mask" style="background-color: rgba(0, 0, 0, 0.4);">
        <div class="d-flex justify-content-center align-items-center h-100">
          <div class="text-white">
            <h1 class="mb-3">Team</h1>
            <h4 class="mb-3">Meet our Team</h4>
            <!-- <a class="btn btn-outline-light btn-lg" href="#!" role="button">Call to action</a> -->
          </div>
        </div>
      </div>
    </div>
    <!-- Background image -->


  </header>

  <!--Main layout-->
  <main class="my-5">
    <div class="container">

      <!--Section: Content-->
      <section class="text-center">
        <h4 class="mb-5"><strong>Team Head</strong></h4>

        <div class="row justify-content-center">
          <% rows.forEach(function(row) { %>
          <% if (row['Designation'] == "Team Head") { %>
          <div class="col-lg-3 col-md-6 mb-4">
            <div class="card">
              <div class="bg-image hover-overlay ripple d-flex" data-mdb-ripple-color="light" style="height: 350px; vertical-align: middle; align-self: center;">
                <img src="<%=row['Photo link']%>" class="img-fluid" />
                <a href="#!">
                  <div class="mask" style="background-color: rgba(251, 251, 251, 0.15);"></div>
                </a>
              </div>
              <div class="card-body">
                <h5 class="card-title"><%=row["Name"]%></h5>
                <div class="h3 container mt-3 " style="vertical-align: middle;">
                  <% if (row['Google scholar'] != "") { %>
                  <a href="<%=row['Google scholar']%>" target="_blank"><i class="ai ai-google-scholar-square ai-1x"></i></a>
                  <% } if (row['Linkedin'] != "") { %>
                  <a href="<%=row['Linkedin']%>" target="_blank"><i class="fab fa-linkedin m-2"></i></a>
                  <% } if (row['ResearchGate'] != "") { %>
                  <a href="<%=row['ResearchGate']%>" target="_blank"><i class="fab fa-researchgate m-2"></i></a>
                  <% } if (row['personal website'] != "") { %>
                  <a href="<%=row['personal website']%>" target="_blank"><i class="far fa-user m-2"></i></a>
                  <% } %>
                </div>
              </div>
            </div>
          </div>
          <% } %>
          <% }); %>

        </div>

